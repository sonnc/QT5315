            <result name="success" type="redirectAction" >
                <param name="actionName">delfilesv</param>     
            </result>
            <result name="error" type="redirect">404.jsp</result>
        </action>
        <action name="delfilesv">
            <result>/pages/sinhvien/tailieu.jsp</result>
